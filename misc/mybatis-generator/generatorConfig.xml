<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="context1" targetRuntime="MyBatis3"> <!-- targetRuntimeをMyBatis3にしないと、Exampleができない -->
        <jdbcConnection driverClass="org.postgresql.Driver"
            connectionURL="jdbc:postgresql://localhost:5432/postgres"
            userId="postgres" password="postgres" />

        <javaModelGenerator targetPackage="dto"
            targetProject="sample-webapp-jsf/src/main/java" />

        <sqlMapGenerator targetPackage="sql"
            targetProject="sample-webapp-jsf/src/main/resources" />

        <javaClientGenerator targetPackage="mapper"
            targetProject="sample-webapp-jsf/src/main/java" type="XMLMAPPER" />

        <table schema="public" tableName="user">
            <property name="useActualColumnName" value="true" />
            <!-- <columnOverride column="???" property="???" /> -->
        </table>
    </context>

</generatorConfiguration>